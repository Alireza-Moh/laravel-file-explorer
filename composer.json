{
    "name": "alireza/laravel-file-explorer",
    "description": "A laravel file manager",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alireza\\LaravelFileExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alireza\\LaravelFileExplorer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alireza Mohammadi"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "league/flysystem": "^3.0",
        "ext-zip": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alireza\\LaravelFileExplorer\\LaravelFileExplorerServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "orchestra/testbench": "9.x-dev"
    }
}
